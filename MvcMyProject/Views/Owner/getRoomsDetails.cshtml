@model IEnumerable<MvcMyProject.Models.RoomModel>
           <head><link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/css/bootstrap.min.css"></head>

<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.roomname)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomrent)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomoccupancy)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomavailability)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomamenities)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomimage1)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.roomimage2)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.roomname)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.roomrent)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.roomoccupancy)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.roomavailability)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.roomamenities)
        </td>
        <td>
            <img src=@item.roomimage1 width="100px" height="100px" />
        </td>
        <td>
            <img src=@item.roomimage2 width="100px" height="100px" />
        </td>
    </tr>
}

</table>
